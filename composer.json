{
    "name": "el7cosmos/elabee",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Abi",
            "email": "el@elabee.me",
            "homepage": "http://elabee.me"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "fortawesome/font-awesome",
                "version": "5.1.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/FortAwesome/Font-Awesome/releases/download/5.1.1/fontawesome-free-5.1.1-web.zip",
                    "type": "zip"
                },
                "extra": {
                    "installer-name": "fontawesome"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twbs/bootstrap",
                "version": "v3.3.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/twbs/bootstrap/releases/download/v3.3.7/bootstrap-3.3.7-dist.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "~7.2.0",
        "ext-apcu": "*",
        "ext-gd": "*",
        "ext-redis": "*",
        "cweagans/composer-patches": "^1.6",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.8",
        "drupal/console-dotenv": "^0.3.0",
        "drupal/core": "~8.6",
        "drupal/fontawesome": "^2.4",
        "drupal/fortytwo_admin": "^1.17",
        "drupal/fortytwo_admin_toolbar": "^1.6",
        "drupal/migrate_plus": "^5.0@beta",
        "drupal/migrate_tools": "^4.0@beta",
        "drupal/minifyhtml": "^1.2",
        "drupal/monolog": "^1.0",
        "drupal/redis": "^1.0",
        "drupal/s3fs": "^3.0@alpha",
        "drush/drush": "^9.5",
        "facebook/graph-sdk": "^5.6",
        "fortawesome/font-awesome": "^5.0",
        "mailgun/mailgun-php": "^2.5",
        "php-http/guzzle6-adapter": "^1.1",
        "sentry/sentry": "^1.9",
        "twbs/bootstrap": "^3.3",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "behat/mink": "~1.7@dev",
        "behat/mink-selenium2-driver": "^1.3@dev",
        "codacy/coverage": "^1.4",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/coder": "^8.2",
        "drupal/config_inspector": "^1.0@beta",
        "drupal/devel": "^1.2",
        "php-coveralls/php-coveralls": "^2.0",
        "sensiolabs/security-checker": "^5.0",
        "webflo/drupal-core-require-dev": "~8.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "apcu-autoloader": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/Heroku.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\Heroku::pgPassFile"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
